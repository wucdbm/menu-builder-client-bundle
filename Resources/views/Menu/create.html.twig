{% extends '@WucdbmMenuBuilderClient/Menu/base.html.twig' %}

{% block content %}

    <div class="row">
        <div class="col-lg-12">
            {% form_theme form '@WucdbmMenuBuilderClient/_shared/form/form_theme.html.twig' %}
            {{ form_start(form) }}
            {{ form_errors(form) }}
            {{ form_rest(form) }}
            <div class="form-group">
                <button type="submit" class="btn btn-default">
                    Create
                </button>
            </div>
            {{ form_end(form) }}
        </div>
    </div>

{% endblock %}

{% block breadcrumb %}
    {{ parent() }}
    <li>
        Create
    </li>
{% endblock %}